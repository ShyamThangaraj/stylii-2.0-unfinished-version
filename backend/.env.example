# Stylii Environment Variables
# Copy this file to .env and fill in your actual API keys

# Google Gemini API Keys
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_API_KEY_2=your_second_gemini_api_key_here

# ElevenLabs API Key
# Get your API key from: https://elevenlabs.io/app/settings/api-keys
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# FAL AI API Key
# Get your API key from: https://fal.ai/dashboard/keys
FAL_KEY=your_fal_ai_api_key_here

# SerpAPI Key
# Get your API key from: https://serpapi.com/dashboard
SERPAPI_KEY=your_serpapi_key_here

# Optional: Database URL (if using a database)
# DATABASE_URL=postgresql://username:password@localhost:5432/stylii

# Optional: Redis URL (if using Redis for caching)
# REDIS_URL=redis://localhost:6379

# Development Settings
NODE_ENV=development
NEXT_PUBLIC_API_URL=http://localhost:8000
